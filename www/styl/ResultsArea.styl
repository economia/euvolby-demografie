&
    position absolute
    top 0px
    left 0px
    .zeroLine
        stroke #6
        shape-rendering crispEdges
    .party
        text
            fill #3
        text.partyName
        text.partyResult
        text.partyDifference
            text-anchor middle
        text.partyName
            font-size 0.8em
            font-variant small-caps
        text.partyResult
        text.partyDifference
            font-size 0.72em
        text.partyDifference
            font-size  0.7em
        // rect.default
        //     opacity 0.5
        rect.diff
            fill #fc8d59
            &.positive
                fill #91cf60

